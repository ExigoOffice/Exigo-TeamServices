{
    "manifestVersion": 1,
    "id": "exigo-user-stories",
    "version": "0.1.32",
    "name": "Exigo User Stories",
    "description": "A management tool for exigo tickets.",
    "publisher": "jacobbass",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "static/images/logo.png"
    },
    "contributions": [
        {
            "id": "Ticket",
            "type": "ms.vss-work-web.work-item-form-page",
            "description": "Adds Ticketing functionality to tfs.",
            "targets": [
                "ms.vss-work-web.work-item-form"
            ],
            "properties": {
                "name": "Ticket",
                "uri": "static/app.html"
            }
        }
    ],
    "scopes": [
        "vso.work",
        "vso.code_write"
    ],
     "files": [ 
        {
            "path": "dist",
            "addressable": true
        },
        {
            "path": "static",
            "addressable": true
        },
        {
            "path": "node_modules/vss-web-extension-sdk/lib",
            "addressable": true,
            "packagePath": "lib"
        }
    ]
}